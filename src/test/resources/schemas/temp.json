{
  "openapi": "3.0.0",
  "info": {
    "title": "API de autenticación",
    "description": "Microservicio de autenticación, el cual permite el registro de usuarios y la generación token usando las credenciales de los usuarios. Usado para ilustrar el tema de seguridad basada en el uso de JWT",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:8090/",
      "description": "Default Server."
    }
  ],
