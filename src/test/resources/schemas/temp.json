        "summary": "Listar tokens",
        "description": "Permite obtener un listado de los token validos",
        "operationId": "TokenAPI :: listar tokens",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Tokens"
                }
              }
            },
            "description": "Operación exitosa"
          },
          "401": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errores"
                }
              }
            },
            "description": "Usuario no autorizado"
          },
          "403": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errores"
                }
              }
            },
            "description": "No tiene permitido realizar la operación"
          },
          "404": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errores"
                }
              }
            },
            "description": "Recurso no encontrado"
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errores"
                }
              }
            },
            "description": "Error interno"
          }
        },
        "deprecated": false,
        "security": [
          {
            "JWT": [
              "user",
              "admin"
            ]
          }
        ]
      },
      "post": {
        "summary": "Generar Token",
        "description": "Permite generar un token a partir de las credenciales (usuario y clave) de un usuario registrado en el sistema",
        "operationId": "TokenAPI :: generar",
        "requestBody": {
          "description": "Credenciales del usuario que desea que se cree el token",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Credential"
              }
            },
            "*/*": {
              "schema": {
                "$ref": "#/components/schemas/Credential"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Token"
                }
              }
            },
            "description": "Operación exitosa"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errores"
                }
              }
            },
            "description": "La solicitud está incompleta, el usuario y la clave son requeridas"
          },
          "401": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errores"
                }
              }
            },
            "description": "Nombre de usuario o clave incorrectas."
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errores"
                }
              }
            },
            "description": "Error interno"
          }
        },
        "deprecated": false
      }
    },
